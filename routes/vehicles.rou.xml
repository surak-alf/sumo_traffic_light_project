<routes>
    <vType id="car" accel="2.6" decel="4.5" length="5" maxSpeed="50"/>
    
    <!-- Routes -->
    <route id="east_west" edges="west_to_center center_to_east"/>
    <route id="west_east" edges="east_to_center center_to_west"/>
    <route id="north_south" edges="north_to_center center_to_south"/>
    <route id="south_north" edges="south_to_center center_to_north"/>
    
    <!-- Traffic flows (vehicles/hour) -->
    <flow id="flow1" type="car" route="east_west" begin="0" end="3600" vehsPerHour="600"/>
    <flow id="flow2" type="car" route="west_east" begin="0" end="3600" vehsPerHour="600"/>
    <flow id="flow3" type="car" route="north_south" begin="0" end="3600" vehsPerHour="400"/>
    <flow id="flow4" type="car" route="south_north" begin="0" end="3600" vehsPerHour="400"/>
</routes>