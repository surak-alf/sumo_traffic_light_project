<?xml version="1.0" encoding="UTF-8"?>
<routes>
    <vType id="car" accel="2.6" decel="4.5" length="5" maxSpeed="50"/>
    
    <!-- Define routes -->
    <route id="west_east" edges="west_to_center center_to_east"/>
    <route id="east_west" edges="east_to_center center_to_west"/>
    <route id="north_south" edges="north_to_center center_to_south"/>
    <route id="south_north" edges="south_to_center center_to_north"/>
    
    <!-- Add vehicles -->
    <flow id="flow_we" type="car" route="west_east" begin="0" end="3600" vehsPerHour="300"/>
    <flow id="flow_ew" type="car" route="east_west" begin="0" end="3600" vehsPerHour="300"/>
    <flow id="flow_ns" type="car" route="north_south" begin="0" end="3600" vehsPerHour="300"/>
    <flow id="flow_sn" type="car" route="south_north" begin="0" end="3600" vehsPerHour="300"/>
</routes>